/////////////////////////////////////////////big photo
class Myhome1 extends StatefulWidget {
  final int index;
  final int j;
  const Myhome1({Key? key, required this.index, required this.j})
      : super(key: key);

  @override
  State<Myhome1> createState() => _Myhome1State();
}

class _Myhome1State extends State<Myhome1> {
  @override
  Widget build(BuildContext context) {
    var size = MediaQuery.of(context).size;
    // ignore: prefer_typing_uninitialized_variables
    return Scaffold(
      backgroundColor: Color.fromARGB(255, 0, 0, 0),
      body: Stack(children: [
        Positioned(
          child: Container(
            height: size.height,
            width: size.width,
            decoration: BoxDecoration(
              borderRadius: BorderRadius.circular(15),
              image: DecorationImage(
                image: NetworkImage(lists[widget.index][widget.j]),
                fit: BoxFit.fill,
              ),
            ),
          ),
        ),
        Positioned(
          bottom: 30,
          left: 70,
          child: Row(
            children: [
              //
              ElevatedButton(
                onPressed: () {
                  setState(() {
                    // writeCounter(lists[widget.index][widget.j]);
                    favoritlist.add(lists[widget.index][widget.j]);
                  });
                },
                style: ButtonStyle(
                  backgroundColor: MaterialStateProperty.all<Color>(
                    Color.fromARGB(255, 182, 180, 176),
                  ),
                ),
                child: const Icon(
                  Icons.favorite,
                  color: Color.fromARGB(255, 0, 0, 0),
                ),
              )
            ],
          ),
        ),
      ]),
    );
  }
}

/////////////////////////random photos//////////////
class Myrandom extends StatelessWidget {
  final int index;
  final int j;
  const Myrandom({Key? key, required this.index, required this.j})
      : super(key: key);
  @override
  Widget build(BuildContext context) {
    var size = MediaQuery.of(context).size;
    // ignore: prefer_typing_uninitialized_variables
    return Scaffold(
      backgroundColor: Color.fromARGB(255, 0, 0, 0),
      body: Container(
        height: size.height,
        width: size.width,
        decoration: BoxDecoration(
          borderRadius: BorderRadius.circular(15),
          image: DecorationImage(
            image: NetworkImage(random[index][j]),
            fit: BoxFit.fill,
          ),
        ),
      ),
    );
  }
}

