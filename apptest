class MyHomePage extends StatefulWidget {
  const MyHomePage({Key? key}) : super(key: key);

  @override
  _MyHomePageState createState() => _MyHomePageState();
}

class _MyHomePageState extends State<MyHomePage> {
  @override
  Widget build(BuildContext context) {
    int i;
    Size size = MediaQuery.of(context).size;
    return Scaffold(
      body: Container(
          // color: Color.fromARGB(255, 172, 164, 164),
          decoration: BoxDecoration(
            //Color.fromARGB(255, 121, 74, 74),
            color: Color.fromARGB(255, 218, 214, 205),
            // Color.fromARGB(255, 121, 74, 74),
          ),
          child: Stack(
            children: [
              Positioned(
                  top: 174,
                  left: 0,
                  width: size.width,
                  height: size.height * .70,
                  child: Container(
                      decoration: BoxDecoration(
                        borderRadius: BorderRadius.circular(15),
                        color: Color.fromARGB(255, 255, 255, 254),
                        // border: Border.all(
                        //     color: Color.fromARGB(255, 172, 164, 164),
                        //     width: 5),
                        // boxShadow: [
                        //   BoxShadow(
                        //     color: Colors.black,
                        //     blurRadius: 5.0,
                        //   )
                        //],
                      ),
                      child: ListWheelScrollView(
                          itemExtent: 500,
                          //  physics: FixedExtentScrollPhysics(),
                          // onSelectedItemChanged: (i) =>
                          //     Center(child: Text((i + 1).toString())),
                          controller:
                              ScrollController(initialScrollOffset: 0.2),
                          perspective: 0.005,
                          diameterRatio: 4,
                          children: [
                            for (i = 0; i < random1.length; i++)
                              (Container(
                                decoration: BoxDecoration(
                                    borderRadius: BorderRadius.circular(15),
                                    //  color: Colors.amber,
                                    boxShadow: [
                                      BoxShadow(
                                        color: Colors.black,
                                        blurRadius: 6.0,
                                      ),
                                    ],
                                    image: DecorationImage(
                                        image: NetworkImage(random1[i]),
                                        fit: BoxFit.fill)),
                              ))
                          ]))),
              Positioned(
                bottom: 0,
                left: 0,
                child: Container(
                  height: size.height * .06,
                  width: size.width,
                  decoration: BoxDecoration(
                    borderRadius: BorderRadius.circular(20),
                    color: Color.fromARGB(255, 255, 252, 252),
                    boxShadow: [
                      BoxShadow(
                        color: Colors.black,
                        blurRadius: 6.0,
                      )
                    ],
                  ),
                ),
              ),
              Positioned(
                top: 0,
                left: 0,
                child: Container(
                  height: size.height * .25,
                  width: size.width,
                  decoration: BoxDecoration(
                    borderRadius: BorderRadius.circular(20),
                    color: Color.fromARGB(255, 255, 255, 255),
                    boxShadow: [
                      BoxShadow(
                        color: Colors.black,
                        blurRadius: 5.0,
                      )
                    ],
                  ),
                ),
              ),
              Positioned(
                top: 50,
                left: 0,
                width: size.width,
                height: size.height * .4,
                child: Container(
                  child: ListView.builder(
                    itemCount: animes.length,
                    scrollDirection: Axis.horizontal,
                    itemBuilder: (context, index) {
                      return (InkWell(
                        child: Padding(
                          padding: const EdgeInsets.only(
                              top: 8, left: 4.0, right: 4.0),
                          child: Column(
                            children: [
                              Container(
                                height: 80,
                                width: 80,
                                decoration: BoxDecoration(
                                    shape: BoxShape.circle,
                                    image: DecorationImage(
                                        image: NetworkImage(animesicons[index]),
                                        fit: BoxFit.fill),
                                    border: Border.all(
                                        color:
                                            Color.fromARGB(255, 194, 187, 187),
                                        width: 5),
                                    boxShadow: [
                                      BoxShadow(
                                        color: Colors.black,
                                        blurRadius: 8.0,
                                        spreadRadius: 1,
                                        offset: Offset(5, 5),
                                      ),
                                      BoxShadow(
                                        color:
                                            Color.fromARGB(255, 255, 255, 255),
                                        blurRadius: 8.0,
                                        spreadRadius: 0.5,
                                        offset: Offset(-5, -5),
                                      )
                                    ]),
                              ),
                              SizedBox(
                                height: 5,
                              ),
                              Center(
                                child: Text(
                                  animes[index].toString(),
                                  style: TextStyle(
                                      color: Color.fromARGB(255, 0, 0, 0),
                                      fontSize: 15,
                                      fontWeight: FontWeight.bold),
                                ),
                              ),
                            ],
                          ),
                        ),
                        onTap: () {
                          Navigator.push(
                            context,
                            MaterialPageRoute(
                                builder: (context) => Myanime(index: index)),
                          );
                        },
                      ));
                    },
                  ),
                ),
              ),
              Positioned(
                bottom: 10,
                left: 20,
                child: InkWell(
                  child: Container(
                    height: 60,
                    width: 60,
                    decoration: BoxDecoration(
                        shape: BoxShape.circle,
                        color: Color.fromARGB(255, 194, 187, 187),
                        border: Border.all(
                            color: Color.fromARGB(255, 255, 255, 255),
                            width: 5),
                        boxShadow: [
                          BoxShadow(
                            color: Colors.black,
                            blurRadius: 8.0,
                            spreadRadius: 1,
                            offset: Offset(5, 5),
                          ),
                          BoxShadow(
                            color: Color.fromARGB(255, 255, 255, 255),
                            blurRadius: 8.0,
                            spreadRadius: 0.5,
                            offset: Offset(-5, 3),
                          )
                        ]),
                    child: Icon(
                      Icons.exit_to_app,
                      color: Color.fromARGB(255, 0, 0, 0),
                    ),
                  ),
                  onTap: () {
                    Navigator.push(
                      context,
                      MaterialPageRoute(
                        builder: (context) => exitapp(),
                      ),
                    );
                  },
                ),
              ),
              Positioned(
                bottom: 10,
                right: 20,
                child: Container(
                  height: 60,
                  width: 60,
                  decoration: BoxDecoration(
                      shape: BoxShape.circle,
                      color: Color.fromARGB(255, 194, 187, 187),
                      border: Border.all(
                          color: Color.fromARGB(255, 255, 255, 255), width: 5),
                      boxShadow: [
                        BoxShadow(
                          color: Colors.black,
                          blurRadius: 8.0,
                          spreadRadius: 1,
                          offset: Offset(5, 5),
                        ),
                        BoxShadow(
                          color: Color.fromARGB(255, 255, 255, 255),
                          blurRadius: 8.0,
                          spreadRadius: 0.5,
                          offset: Offset(-5, 3),
                        )
                      ]),
                  child: Icon(
                    Icons.download,
                    color: Color.fromARGB(255, 0, 0, 0),
                  ),
                ),
              ),
              Positioned(
                bottom: 10,
                left: size.width * .4,
                child: InkWell(
                  child: Container(
                    height: 60,
                    width: 60,
                    decoration: BoxDecoration(
                        shape: BoxShape.circle,
                        color: Color.fromARGB(255, 194, 187, 187),
                        border: Border.all(
                            color: Color.fromARGB(255, 255, 255, 255),
                            width: 5),
                        boxShadow: [
